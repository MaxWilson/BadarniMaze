{
  "module": "commonjs",
  "sourceMaps": true,
  "projFile": "./Beast.fsproj",
  "refs": {
        "Fable.Core": "fable-core"
  },
  "scripts": {
    "postbuild-once": "node node_modules/webpack-dev-server/bin/webpack-dev-server.js --colors --hot --config webpack.dev.config --content-base public/"
  },
  "targets": {
    "prod": {
      "scripts": {
        "postbuild-once": ""
      },
      "production": true
    },
    "watch": {
      "scripts": {
        "postbuild-once": "node node_modules/webpack-dev-server/bin/webpack-dev-server.js --colors --hot --config webpack.dev.config --content-base public/"
      },
      "watch": true
    }
  },
  "rollup": {
    "entry": "./source/main.js",
    "dest": "./out/bundle.js",
    "external": ["react", "react-dom"],
    "globals": {
        "react": "React",
        "react-dom": "ReactDOM"
    }
  }
}